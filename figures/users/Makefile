# userstats-bridge-transport-multi.csv and
# userstats-bridge-combined-multi.csv are generated by the scripts at
# https://gitlab.torproject.org/dcf/snowflake-graphs

all: \
	users-global.pdf \
	users-ru.pdf
.PHONY: all

users-global.pdf: .EXTRA_PREREQS = users-global.r ../common.r
users-global.pdf: userstats-bridge-transport-multi.csv
	Rscript users-global.r "$<" "$@"

users-ru.pdf: .EXTRA_PREREQS = users-ru.r ../common.r
users-ru.pdf: userstats-bridge-combined-multi.csv
	Rscript users-ru.r "$<" "$@"

.DELETE_ON_ERROR:
